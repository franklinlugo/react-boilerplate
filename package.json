{
  "name": "react-boilerplate-with-webpack",
  "version": "0.1.0",
  "description": "react boilerplate with webpack in order to have a straightforward scaffold to practice",
  "main": "src/entries/index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.dev.config.js --mode development --open 'google-chrome'",
    "build:local": "webpack --env.NODE_ENV=local",
    "build:prod": "webpack -p --env.NODE_ENV=production",
    "format": "prettier-eslint --single-quote --print-width=120 --write webpack.dev.config.js \"src/**/*.{js,jsx}\"",
    "lint": "eslint **/*.{js,jsx} --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franklinlugo/react-boilerplate-with-webpack.git"
  },
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "Franklin Lugo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/franklinlugo/react-boilerplate-with-webpack/issues"
  },
  "homepage": "https://github.com/franklinlugo/react-boilerplate-with-webpack#readme",
  "dependencies": {
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "1.0.1",
    "css-loader": "2.1.0",
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "3.0.1",
    "prettier": "1.16.4",
    "prettier-eslint-cli": "4.7.1",
    "style-loader": "0.23.1",
    "webpack": "4.29.1",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14"
  }
}
