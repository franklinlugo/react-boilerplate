{
  "name": "react-boilerplate-with-webpack",
  "version": "0.1.0",
  "description": "react boilerplate with webpack in order to have a straightforward scaffold to practice",
  "main": "src/entries/index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.dev.config.js --mode development --open 'google-chrome'",
    "build:local": "webpack --env.NODE_ENV=local",
    "build:prod": "webpack -p --env.NODE_ENV=production",
    "format": "prettier-eslint --single-quote --print-width=120 --write webpack.dev.config.js \"src/**/*.{js,jsx}\"",
    "lint": "eslint **/*.{js,jsx} --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franklinlugo/react-boilerplate-with-webpack.git"
  },
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "Franklin Lugo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/franklinlugo/react-boilerplate-with-webpack/issues"
  },
  "homepage": "https://github.com/franklinlugo/react-boilerplate-with-webpack#readme",
  "dependencies": {
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "6.0.0",
    "prettier": "2.0.2",
    "prettier-eslint-cli": "5.0.0",
    "style-loader": "1.1.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
}
