    "lint": "eslint **/*.{js,jsx} --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franklinlugo/react-boilerplate-with-webpack.git"
  },
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "Franklin Lugo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/franklinlugo/react-boilerplate-with-webpack/issues"
  },
  "homepage": "https://github.com/franklinlugo/react-boilerplate-with-webpack#readme",
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-react": "7.10.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "css-loader": "3.6.0",
    "eslint": "7.4.0",
    "eslint-config-airbnb": "18.2.0",
